<h1><%= @genre.name %></h1>
<div>
    <h2>Songs:</h2>
    <ul>
    <% @genre.songs.each do |song|%>
        <li><%= link_to song.name, song_path(song) %></li>
        <% end%>
    </ul>
</div>
<div>
    <h2>Artists:</h2>
    <ul>
    <% @genre.songs.each do |song|%>
        <li><%= link_to song.artist.name, artist_path(song.artist) %></li>
        <% end%>
    </ul>
</div>
<p><%= button_to "All Genres", genres_path, :method => :get %></p>